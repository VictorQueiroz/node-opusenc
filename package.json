{
    "dependencies": {
        "bindings": "^1.5.0",
        "cmake-js": "7.x",
        "nan": "2.x"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/victorqueiroz/node-opusenc"
    },
    "main": "./index.js",
    "files": [
        "src/**/*.{cpp,h}",
        "*.{js,cpp,c,h,d.ts}",
        "!test/*.ogg",
        "speexdsp-cmake/**/*.{in,txt,cmake,ac,h,c,am,mk}",
        "libopusenc-cmake/**/*.{in,txt,cmake,ac,h,c,am,mk}",
        "opus/**/*.{in,txt,cmake,ac,h,c,am,mk}",
        "CMakeLists.txt"
    ],
    "scripts": {
        "test": "npx ts-node test",
        "compile:debug": "yarn run compile -D",
        "compile": "npx cmake-js compile -C",
        "prepublishOnly": "npx ts-node scripts/generate-constants.ts && yarn run postinstall && yarn test",
        "postinstall": "npx cmake-js compile -C --CDCMAKE_EXPORT_COMPILE_COMMANDS=1"
    },
    "keywords": [
        "opus",
        "opusenc",
        "libopusenc",
        "audio",
        "libopus",
        "speexdsp",
        "voice",
        "voip"
    ],
    "license": "MIT",
    "name": "libopus",
    "version": "0.0.30",
    "devDependencies": {
        "@types/node": "18.x",
        "chalk": "4.x",
        "codestreamjs": "^0.0.1",
        "glob": "9.x",
        "prettier": "2.x",
        "rxjs": "7.x",
        "ts-node": "10.x",
        "typescript": "5.x"
    }
}
